{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    // "rootDir": "./src",
    "moduleResolution": "bundler",
    // "baseUrl": "./src",
    // "paths": {
    //   "@/*": ["*"]
    // },
    "allowImportingTsExtensions": true,
    "outDir": "./dist",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
